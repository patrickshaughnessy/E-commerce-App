  "scripts": {
    "start": "node .",
    "start:dev": "NODE_ENV=development concurrently -r \"npm run start:dev:server-node\" \"npm run start:dev:client-js\" \"npm run start:dev:client-css\" \"npm run start:dev:server-js\"",
    "start:dev:server-node": "nodemon",
    "start:dev:client-js": "parcel watch src/js/client.js -d dist/ --public-url \"/\" --target browser --no-autoinstall",
    "start:dev:client-css": "parcel watch src/css/styles.less src/css/favicon.ico -d dist/ --public-url \"/\" --target browser --no-autoinstall",
    "start:dev:server-js": "parcel watch src/js/server.js -d dist/ --public-url \"/\" --target node --no-autoinstall",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rm -rf .cache && rm -rf dist",
    "generate:all": "node ./data/utils/generate"
  },
