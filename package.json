{
  "name": "paid_course",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "10.x",
    "npm": "6"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node .",
    "start:dev": "NODE_ENV=development concurrently -r \"npm run start:dev:server-node\" \"npm run start:dev:client-js\" \"npm run start:dev:server-js\"",
    "start:dev:server-node": "nodemon",
    "start:dev:client-js": "parcel watch src/js/client.js src/css/styles.less src/css/favicon.ico -d dist/ --public-url ./ --target browser --no-autoinstall --no-source-maps",
    "start:dev:server-js": "parcel watch src/js/server.js -d dist/ --public-url ./ --target node --no-autoinstall --no-source-maps",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production parcel build src/js/client.js src/css/styles.less src/css/favicon.ico -d dist/ --public-url ./ --target browser --no-autoinstall --no-source-maps --detailed-report",
    "build:server": "NODE_ENV=production parcel build src/js/server.js -d dist/ --public-url ./ --target node --no-autoinstall --no-source-maps --detailed-report",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rm -rf .cache && rm -rf dist",
    "generate": "node ./data/mockData/generate",
    "populate": "node ./data/mockData/populate"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cheerio": "^1.0.0-rc.2",
    "connect-mongo": "^2.0.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.0",
    "jquery": "^3.3.1",
    "loading-css": "^0.2.0",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^1.0.6",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serve-static": "^1.13.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "concurrently": "^4.0.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "less": "^3.8.1",
    "nodemon": "^1.18.5",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.3.11"
  }
}
