{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 1
  }
}
